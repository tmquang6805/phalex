{
    "name" : "tmquang6805/phalex",
    "description" : "Library build on top PhalconPHP Framework",
    "type" : "library",
    "keywords" : [
        "phalcon",
        "library",
	"extension"
    ],
    "license" : "MIT",
    "authors" : [
        {
            "name" : "Tran Minh Quang",
            "email" : "tmquang6805@yahoo.com",
            "homepage" : "http://tmquang6805.blogspot.com"
        }
    ],
    "require" : {
        "php" : ">=5.5",
        "ext-phalcon" : "~1.3",
        "zendframework/zend-config" : "~2.3",
        "symfony/console": "~2.6"
    },
    "require-dev" : {
        "fabpot/php-cs-fixer" : "~1.0",
        "squizlabs/php_codesniffer": "~1.0",
        "phpunit/PHPUnit" : "~4.4",
        "sebastian/phpcpd": "~2.0",
        "mockery/mockery" : "*"
    },
    "autoload" : {
        "psr-4" : {
            "Phalex\\" : "library/Phalex/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "PhalexTest\\" : "tests/PhalexTest/"
        }
    }
