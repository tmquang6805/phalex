{
    "name": "tmquang6805/phalcon-ext",
    "description": "Phalcon extension library for develop multi modules easier",
    "version": "0.5.0",
    "type": "library",
    "keywords": [
        "phalcon",
        "module"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Tran Minh Quang",
            "email": "tmquang6805@yahoo.com",
            "homepage": "http://tmquang6805.blogspot.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-phalcon": "~1.3"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "1.0",
        "phpunit/PHPUnit": "~4.3"
    },
    "autoload": {
        "psr-4": {
            "Phalcon\\Extension\\": "library/Phalcon/Extension/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phalcon\\ExtensionTest\\": "tests/Phalcon/ExtensionTest/"
        }
    }
}
